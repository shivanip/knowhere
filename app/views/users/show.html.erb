<div style="height: 100px;" id="hero-unit" class="hero-unit"></div>
<div id="hero-unit" class="hero-unit">
<div class="col-sm-4 col-sm-offset-4 panel panel-default">
<div style="height: 20px"></div>
  <div class="panel-heading">
    <h1 class="text-center"><%= @user.username %></h1>
  </div>
  <div class="panel-body">
    <div class="secret-message">Find <%= @user.username %>'s secrets.
    </div>
  </div>
  <%= content_tag "div", id: "map-canvas", data: {url: user_url(@user)}, style: "height: 325px; width: 100%" do %>
  <% end %>
  <% if current_user && user_authorized? %>
    <div style="height: 20px;"></div>
    <div class="submit_button text-center"><%= link_to "Edit Profile", edit_user_path(@user), id: "btn-contact", class: "btn btn-primary btn-lg" %></div>
  <% end %>
</div>
</div>